[MASTER]
disable=
    C0111, # missing-docstring
    C0103, # invalid-name
    C0303, # trailing-whitespace
    C0330, # wrong-hanging-indentation
    W0611, # unused-import (pycln handles this)
    R0903, # too-few-public-methods
    R0913, # too-many-arguments

[FORMAT]
max-line-length=88

[MESSAGES CONTROL]
disable=C0111,C0103

[REPORTS]
output-format=colorized